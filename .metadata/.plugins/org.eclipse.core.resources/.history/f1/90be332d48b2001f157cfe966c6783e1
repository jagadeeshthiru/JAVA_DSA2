package DAY2;

public class PAT {
  public static void main(String[] args) {
	int rows=5,cols=5;
	for(int r=1;r<=rows;r++)
	{
		int temp=1;
		for(int c=cols;c>=1;c--)
		{
			if(r%2!=0)
			{
				System.out.print(temp+" ");
				temp++;
			}
			else
			{
				System.out.print(c+" ");
			}
		}
		System.out.println();
	}
}
}
