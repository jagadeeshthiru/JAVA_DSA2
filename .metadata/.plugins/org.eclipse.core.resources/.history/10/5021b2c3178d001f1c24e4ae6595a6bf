package Practice;

public class Sort1 {
public static void main(String[] args) {
	int a[]=new int[] {500,22,33,44,56,30};
	for (int i = 0; i < a.length; i++) {
		
		for (int j = i+1; j < a.length; j++) {
			if(a[i]>a[j])
			{
				int t=a[i];
				a[i]=a[j];
				a[j]=t;
			}
			
		}
	}
	for (int temp : a) {
		System.out.println(temp);
		
	}
}
}
