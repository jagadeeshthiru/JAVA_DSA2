package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=5,cols=5,temp=rows+1;
	
	for(int r=1;r<=rows;r++)
	{
	    for(int c=cols;c>=r;c--)
	    {
	    	if(c==1||r+c==temp)
	    	System.out.print("* ");
	    	else
	    		System.out.print("   ");
	    }
	    
	    System.out.println();
	}
}
}
