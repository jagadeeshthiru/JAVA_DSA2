package DAY2;

public class DEMO {
public static void main(String[] args) {
	int rows=5;
	for(int r=1;r<=rows;r++)
	{
		for(int c=1;c<=r;)
		{
			
			int n=2,temp=n;
				int rev=0;
				while(n!=0)
				{
					int rem=n%10;
					rev=rev*10+rem;
					n=n/10;
				}
				
				
			
		 if(temp==rev)
		{
			System.out.print(temp+" ");
		}
		 c++;
		}
		System.out.println();
	}
}
}
