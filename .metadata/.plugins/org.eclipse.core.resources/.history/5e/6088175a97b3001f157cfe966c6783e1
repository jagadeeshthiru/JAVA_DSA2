package DAY2;

public class PAT {
  public static void main(String[] args) {
	for(int x=2;x<=50;x++)
	{
		int n=x,count=0,num;
		for(int y=2;y<=n;y++)
		{
			
			if(n%y==0)
			{
				count++;
			}
			
		}
		if(count==1)
		{ 
			int z=2;
			for(;z<=50;)
			System.out.println(n);
			z=z+3;
		}
		
	}
}
}
