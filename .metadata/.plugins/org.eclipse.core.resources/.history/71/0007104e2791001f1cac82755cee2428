package com;

public class Duplicate {
	public static void main(String[] args) {
		String s="Hello Al123l";
		char a[]=s.toCharArray();
		
		for (int i = 0; i < a.length; i++) {
			int count=0;	
        for (int j = i+1; j < a.length; j++) {
	             if(a[i]==a[j])
	             {
	            	 count++;
	            	 System.out.println(a[j]);
	            	 System.out.println(count);
	             }
	
               }
				
			
			
		}
   }
}
