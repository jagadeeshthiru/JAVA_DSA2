package DAY1;

public class Main2 {
public static void main(String[] args) {
	int a[]=new int[] {1,2,3,4,5};
	int b[]=new int[a.length],sum=0;
	for(int x=1;x<=a.length-1;x++)
	{
		sum=sum+a[x];
		if(x<a.length)
		{
			b[x]=sum-1;
			
		}
			
		System.out.println(b[x]);
	}
	System.out.println(sum);
	
	

}
}
