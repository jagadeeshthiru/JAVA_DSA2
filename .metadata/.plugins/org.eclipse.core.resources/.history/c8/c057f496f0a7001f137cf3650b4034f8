package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=5,cols=5;
	char ch='A';
	char ch1='a';
	for(int r=1;r<=rows;r++)
	{
	    for(int c=1;c<=cols;c++)
	    {
	    	if(r%2==0)
	    	{
	    		System.out.print(ch1+" ");
	    		ch1++;
	    	}
	    	else
	    	{
	    		System.out.print(ch+" ");
	    		ch++;
	    	}
	    	
	    }
	    
	    System.out.println();
	}
}
}
