package Practice;

public class Sort1 {
public static void main(String[] args) {
	int a[]=new int[] {500,22,33,44,56,30};
	for (int i = 0; i < a.length-1; i++) {
		
	
			if(a[i]>a[i+1])
			{
				int t=a[i];
				a[i]=a[i+1];
				a[i+1]=t;
				i=-1;
				
			}
			
		
	}
	for (int temp : a) {
		System.out.println(temp);
		
	}
}
}
