package DAY2;

public class Arr {.
 
	public static void main(String[] args) {
		
		int rows=5;
		for(int r=0;r<=rows;r++)
		{
			for(int c=0;c<=r;c++)
			{
				
				if(r==5||c==1||r==c)
				{
				System.out.print("$  ");
				}
				else
				{
					System.out.print("   ");
				}
			}
			System.out.println();
		}
	}
}
