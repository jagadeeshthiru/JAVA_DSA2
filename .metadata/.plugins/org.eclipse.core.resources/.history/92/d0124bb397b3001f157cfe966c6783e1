package DAY2;

public class PAT {
  public static void main(String[] args) {
	for(int x=2;x<=50;x++)
	{
		int n=x,count=0,num;
		for(int y=2;y<=n;y++)
		{
			
			if(n%y==0)
			{
				count++;
			}
			
		}
		if(count==1)
		{ 
			for(int y=2;y<=50;y=y+3)
			{
			System.out.println(n);
			}
			
		}
		
	}
}
}
