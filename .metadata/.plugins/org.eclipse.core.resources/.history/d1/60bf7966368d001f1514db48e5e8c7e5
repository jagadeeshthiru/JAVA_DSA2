package Practice;

public class Armstrng {
public static void main(String[] args) {
	int a[]=new int[]{153,220,370,120,760,9474};
	int count=0;
	for (int i = 0; i < a.length; i++) {
		int rem=0,rev=0,num=a[i],temp=num,pow=0;
		while(num!=0)
		{
			rem=num%10;	
			count++;
			num=num/10;
		}
		while(num!=0)
		{		
		rem=num%10;
	    pow=(int)Math.pow(num,count);
		num=num/10;
		}
		
	}
	for (int temp : a) {
		System.out.println(temp);
	}
}
}
