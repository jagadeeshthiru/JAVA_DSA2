package com;

public class ToChar {
public static void main(String[] args) {
	String s="Hello Al123l";
	char a[]=s.toCharArray();
	for (int i = 0; i < a.length; i++) {
		int b=(int)a[i];
		if(b>=0&&b<=9)
		{
			System.out.println(a[i]);
		}
//		if(a[i]!=' ')
//		{
//		System.out.println(a[i]);
//		}
	}
		
	}
	
}
