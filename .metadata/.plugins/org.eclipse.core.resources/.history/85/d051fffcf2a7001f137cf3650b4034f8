package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=5,cols=5;
	
	for(int r=1;r<=rows;r++)
	{
	    for(int c=cols;c>=r;c--)
	    {
	    	if(r==1||r==c)
	    	System.out.print("* ");
	    	else
	    		System.out.print("  ");
	    }
	    
	    System.out.println();
	}
}
}
