package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=5,cols=5;
	for(int r=1;r<=5;r++)
	{
	    for(int c=1;c<=5;c++)
	    {
	    	if(r%2==0)
	    	{
	    		System.out.print("$ ");
	    	}
	    	else
	    	{
	    		System.out.print("# ");
	    	}
	    }
	    System.out.println();
	}
}
}
