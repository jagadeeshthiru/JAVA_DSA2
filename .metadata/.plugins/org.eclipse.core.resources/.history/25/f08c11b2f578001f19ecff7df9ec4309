package Extraction;

public class MAXandSmax {
	public static void main(String[] args) {
		int num=1682876,max=0,rem=0,rev=0;
	while(num!=0)
		{
			rem=num%10;//
			
			if(max>=rem) //6
			{
				rem=max;
				System.out.println(" the maximum is"+rem);
			}
			System.out.println(rem);
			num/=10;
		}
	
	}

}
