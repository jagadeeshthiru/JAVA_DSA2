package DAY2;

public class Arr {
 
	public static void main(String[] args) {
		
		int rows=5;
		for(int r=1;r<=rows;r++)
		{
			for(int c=1;c<=r;c++);
			{
				for(int n=1;n<=40;n++)
				{
				int rem=0,rev=0,temp=n;
				   while(n!=0)
				   {
					rem=n%10;
					rev=rev*10+rem;
					n=n/10;
				   }
				   n=temp;
				   if(n==rev)
				   {
					   System.out.print(n+" ");
				   }
				}
				
			}
			System.out.println();
			
		}
	}
}
