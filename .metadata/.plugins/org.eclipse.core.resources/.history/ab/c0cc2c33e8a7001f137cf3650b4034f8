package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=3,cols=6;
	for(int r=1;r<=5;r++)
	{
	    for(int c=1;c<=5;c++)
	    {
	    	if(r==1||r==rows||c==1||c==rows)
	    		
	    	{
	    		System.out.print("# ");
	    	}
	    	else
	    	{
	    		System.out.print("  ");
	    	}
	    }
	    System.out.println();
	}
}
}
