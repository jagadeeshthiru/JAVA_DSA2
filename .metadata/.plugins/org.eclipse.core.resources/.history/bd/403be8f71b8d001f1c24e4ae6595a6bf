package Practice;

public class Searching {
public static void main(String[] args) {
	int a[]=new int[] {10,22,3,45,6,45},search=45;
	for (int i = 0; i < a.length; i++) {
		int count=0;
		if(a[i]==search)
		{
			count++;
			
		}
		
	}
	System.out.println(search+" :is found in array element"+count++);

}
}
