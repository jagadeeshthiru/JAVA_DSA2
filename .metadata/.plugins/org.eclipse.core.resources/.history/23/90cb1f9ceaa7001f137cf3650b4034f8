package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=5,cols=5,temp=rows+1;
	for(int r=1;r<=rows;r++)
	{
	    for(int c=cols;c>=1;c--)
	    {
	    	if(r%2==0)
	    	{
	    	System.err.print(c+" ");
	    	}
	    	else
	    	{
	    		System.out.println(r++);
	    	}
	    }
	    System.out.println();
	}
}
}
