package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=3,cols=10;
	for(int r=1;r<=rows;r++)
	{
	    for(int c=1;c<=cols;c++)
	    {
	    	if(c==r)
	    	{
	    		System.out.println("$ ");
	    	}
	    	else
	    	{
	    		System.out.println("# ");
	    	}
	    }
	    System.out.println();
	}
}
}
