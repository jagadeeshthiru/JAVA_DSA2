package Pattern;

public class PAT {
public static void main(String[] args) {
	int rows=4,cols=4,temp=rows+1,num=2,cum=1;
	for(int r=1;r<=rows;r++)
	{
	    
	    for(int c=1;c<=cols;c++)
	    {
	    	if(r==c)
	    	{
	    		System.out.print(cum+" ");
	    		cum=cum+2;
	    	}
	    }
	    
	    System.out.println();
	}
}
}
